{
  "name": "developer-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "encoding": "^0.1.12",
    "faunadb": "^2.7.0",
    "googleapis": "^43.0.0",
    "jquery": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "3.1.0",
    "swiper": "^6.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-server": "netlify-lambda -c ./src/webpackConfig/webpack.config.js build src/server",
    "test-server": "netlify-lambda -c ./src/webpackConfig/webpack.config.js serve src/server",
    "build-all": "concurrently \"npm run build-server\" \"npm run build\"",
    "test-all": "concurrently \"npm run test-server\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^1.19.2"
  }
}
